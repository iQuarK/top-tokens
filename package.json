  "description": "Application to fetch and display top cryptocurrency tokens",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "dev:consumer": "ts-node-dev --env-file=.env --respawn ./consumer/src/index.ts",
    "dev:server": "ts-node-dev --env-file=.env --respawn ./server/src/index.ts",
    "dev": "ts-node-dev --respawn src/main.ts",
