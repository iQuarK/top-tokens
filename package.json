  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iQuarK/top-tokens/issues"
  },
  "homepage": "https://github.com/iQuarK/top-tokens#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "redis": "^4.7.0",
    "redis-server": "^1.2.2",
    "socket.io": "^4.5.1",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
